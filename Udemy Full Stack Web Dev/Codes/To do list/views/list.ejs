<%- include("header") -%>
<div class="box" id="heading">
  <h1><%= listTitle %></h1>
</div>
<div class="box">
  <% for (var i = 0; i< items.length; i++) { %>
  <div class="item">
    <input type="checkbox" />
    <p><%= items[i] %></p>
  </div>
  <% } %>
  <form class="item" action=" <%= route %> " method="post">
    <input
      type="text"
      name="newItem"
      placeholder="New item"
      autocomplete="off"
    />
    <button type="submit">+</button>
  </form>
</div>
<%- include("footer") -%>
